import java.util.*; 


class reversename
{
	String s;
	public void set(String s)
	{
		this.s=s;
	}
	
public void word()
	{
		s=' '+s;int c1=0,l=s.length(),i,j=0;
		for(i=l-1;i>=0;i--)
		{
				if(s.charAt(i)==' ' )
				{
					System.out.print(s.substring(i,l));
					l=i;
				}
		}
				
	}
}
class rev
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
		reversename obj=new reversename();
		obj.set(s);
		
		obj.word();
	}
}

