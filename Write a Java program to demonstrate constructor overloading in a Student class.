class Student
{
    String name;
    int age;

    // Constructor 1
    Student(String name, int age)
    {
        this.name = name;
        this.age = age;
    }

    // Default Constructor (Fixed)
    Student()
    {
        this("Default Name", 18); // Providing default values
    }

    void display()
    {
        System.out.println("Student: " + name + ", Age: " + age);
    }
}

class c
{
    public static void main(String[] args) 
    {
        Student s1 = new Student("Sneha", 20);
        Student s2 = new Student(); // Calls the default constructor
        s1.display();
        s2.display();
    }
}
